---
import AdminLayout from "@layouts/admin/AdminLayout.astro";
import TabContainer from "@components/TabContainer.astro";

export interface Props {
    active: number;
}
---

<AdminLayout>
    <TabContainer 
        items={[
            { name: "Banner", url: "/admin/content/banners" },
            { name: "Redes Sociales", url: "/admin/content/web-socials" }
        ]}
        active={Astro.props.active}
    />
    <slot/>
</AdminLayout>
